  <filter fluent.**>
    @type record_transformer
    enable_ruby
    <record>
      level ${tag_parts[1] == "warn" ? "warning" : tag_parts[1] == "error" ? "err" : tag_parts[1] == "fatal" ? "crit" : tag_parts[1]}
    </record>
  </filter>
  <filter **>
    @type record_transformer
    enable_ruby
    <record>
      level ${level == "ERROR" ? "err" : level.downcase}
#      fluentd:received_at ${Time.now.utc.strftime("%Y-%m-%dT%H:%M:%S.%6N")}
    </record>
  </filter>
